<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
												   xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
												   android:layout_height="match_parent">

	<TextView
			android:text="TextView"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:id="@+id/itemName"
			tools:layout_editor_absoluteX="83dp" app:layout_constraintTop_toTopOf="parent"
			android:layout_marginTop="8dp" app:layout_constraintBottom_toBottomOf="parent"
			android:layout_marginBottom="8dp" app:layout_constraintVertical_bias="0.265"/>
	<ImageView
			android:layout_width="46dp"
			android:layout_height="45dp" app:srcCompat="@drawable/ic_launcher_background"
			android:id="@+id/imageView2" tools:layout_editor_absoluteY="179dp"
			app:layout_constraintStart_toEndOf="@+id/itemName" android:layout_marginStart="16dp"/>
	<View
			android:id="@+id/divider"
			android:layout_width="100dp"
			android:layout_height="1dp"
			android:background="?android:attr/listDivider"
			tools:layout_editor_absoluteX="83dp"
			app:layout_constraintTop_toBottomOf="@+id/itemName" android:layout_marginTop="24dp"
			app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="16dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>